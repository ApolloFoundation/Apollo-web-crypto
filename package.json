{
  "name": "apl-web-crypto",
  "version": "1.0.19",
  "description": "JS implementation of APL Crypto",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "yarn generate-apollo-api-v2 && jest --config jest.config.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "prepublishOnly": "yarn build",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags",
    "generate-apollo-api-v2": "openapi-generator generate -i https://raw.githubusercontent.com/ApolloFoundation/Apollo/zpl-stage/apl-api2/src/main/resources/yaml/apollo-api-v2.yaml -g typescript-node -o ./src/apollo-api-v2 --additional-properties supportsES6=true && replace-in-file --configFile=replace-config.js"
  },
  "keywords": [
    "First Bridge",
    "Crypto"
  ],
  "author": "Ellina Kolisnichenko",
  "license": "ISC",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "1.0.15-4.3.1",
    "@types/crypto-js": "3.1.47",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "26.0.7",
    "@types/jsbn": "1.2.29",
    "@types/node": "14.0.25",
    "@types/pvutils": "0.0.2",
    "@types/request": "2.48.5",
    "fs": "0.0.2",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "replace-in-file": "6.1.0",
    "request": "2.88.2",
    "ts-jest": "26.1.3",
    "tslint": "6.1.2",
    "typescript": "3.9.7"
  },
  "files": [
    "lib/**/*"
  ],
  "moduleDirectories": [
    "node_modules"
  ],
  "dependencies": {
    "crypto-js": "4.0.0",
    "isomorphic-fetch": "2.2.1",
    "js-sha256": "0.9.0",
    "jsbn": "1.1.0",
    "pako": "1.0.11",
    "pvutils": "1.0.17"
  },
  "directories": {
    "lib": "lib"
  }
}
