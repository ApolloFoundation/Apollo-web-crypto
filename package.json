{
  "name": "apl-web-crypto",
  "version": "1.0.0",
  "description": "JS implementation of APL Crypto",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "prepublishOnly": "yarn test",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "First Bridge",
    "Crypto"
  ],
  "author": "Ellina Kolisnichenko",
  "license": "ISC",
  "devDependencies": {
    "@types/crypto-js": "3.1.44",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "25.2.1",
    "@types/jsbn": "1.2.29",
    "@types/node": "13.13.4",
    "@types/pvutils": "0.0.1",
    "fs": "0.0.1-security",
    "jest": "25.4.0",
    "prettier": "2.0.5",
    "ts-jest": "25.4.0",
    "tslint": "6.1.2",
    "typescript": "3.8.3"
  },
  "files": [
    "lib/**/*"
  ],
  "moduleDirectories": [
    "node_modules"
  ],
  "dependencies": {
    "crypto-js": "4.0.0",
    "isomorphic-fetch": "2.2.1",
    "js-sha256": "0.9.0",
    "jsbn": "1.1.0",
    "node-webcrypto-ossl": "2.0.6",
    "pako": "1.0.11",
    "pvutils": "1.0.17"
  },
  "directories": {
    "lib": "lib"
  }
}
